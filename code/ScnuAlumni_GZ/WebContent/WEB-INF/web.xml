    <servlet-name>ImageCodeServlet</servlet-name>
    <url-pattern>/ImageCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SendPosterServlet</servlet-name>
    <servlet-class>com.newttl.scnualumni_gz.servlet.SendPosterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendPosterServlet</servlet-name>
    <url-pattern>/SendPosterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ManagerLoginServlet</servlet-name>
  	<servlet-class>com.newttl.scnualumni_gz.servlet.ManagerLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerLoginServlet</servlet-name>
    <url-pattern>/ManagerLoginServlet</url-pattern>
  </servlet-mapping>
</web-app>