    <servlet-class>com.newttl.scnualumni_gz.servlet.OAuthServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OAuthServlet</servlet-name>
    <url-pattern>/OAuthServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WxServlet</servlet-name>
    <servlet-class>com.newttl.scnualumni_gz.servlet.WxServlet</servlet-class>
  </servlet>
