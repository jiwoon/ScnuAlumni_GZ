    <servlet-name>SignUpServlet</servlet-name>
    <url-pattern>/SignUpServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ImageCodeServlet</servlet-name>
    <servlet-class>com.newttl.scnualumni_gz.servlet.ImageCodeServlet</servlet-class>
  </servlet>
